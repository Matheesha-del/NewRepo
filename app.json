{
  "expo": {
    "name": "MedTranslator",
    "slug": "MedTranslator",
    "version": "1.0.0",
    "scheme": "MedTranslator",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "plugins": [
      "expo-router",
      "expo-font",
       
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/Micon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ss.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.MedTranslator"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3756ae89-1296-4c47-bb73-ba6e9ea48bf8"
      }
    }
  }
}
